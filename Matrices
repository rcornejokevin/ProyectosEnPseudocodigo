Algoritmo SumaMatrices
	Definir matriz Como Entero
	Dimensionar matriz[3,3]
    Definir i, j, suma Como Entero
    suma <- 0
	

    Para i <- 1 Hasta 3
        Para j <- 1 Hasta 3
            Escribir "Ingrese el valor para la posiciÃ³n (", i, ",", j, "):"
            Leer matriz[i,j]
        FinPara
    FinPara
	

    Para i <- 1 Hasta 3
        Para j <- 1 Hasta 3
            suma <- suma + matriz[i,j]
        FinPara
    FinPara
	
    Escribir "La suma de todos los elementos de la matriz es: ", suma

FinAlgoritmo
